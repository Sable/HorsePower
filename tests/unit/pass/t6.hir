        d1:date = (2010-09-01):date;
        d2:date = (2010-09-01, 1010-01-31, 3019-12-29):date;

        m0:month = 2010-09:month;
        m1:month = (2010-09):month;
        m2:month = (2010-09, 2010-10, 2010-11):month;

        w0:minute = 20:15:minute;
        w1:minute = (20:15):minute;
        w2:minute = (20:15, 21:00, 01:59):minute;

        v0:second = 17:06:20:second;
        v1:second = (17:06:20):second;
        v2:second = (17:06:20, 12:10:01, 09:10:12):second;

        t0:time = 11:22:33.001:time;
        t1:time = (11:22:33.001):time;
        t2:time = (11:22:33.001, 22:33:11.999, 01:02:03.123):time;

        z0:dt = 2019-01-02T17:10:21.001:dt;
        z1:dt = (2019-01-02T17:10:21.001):dt;
        z2:dt = (2019-01-02T17:10:21.001, 2019-12-02T17:01:21.001, 2019-01-02T17:29:21.534):dt;
    }

