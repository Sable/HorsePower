// 31 : udchain while + break

module udchain_while{
    def main():i32{
        x:bool = 1:bool;  // S0
        var z:i32;        // S1
        while(x){         // S2
            x = 0:bool;   // S3
            if(x) break;  // S4
            z = 0:i32;    // S5
        }
        return z;         // S6
    }
}


